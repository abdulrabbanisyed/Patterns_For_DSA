   "source": [
    "def Increasing_Number_Pattern(n):\n",
    "    for i in range(n+1):\n",
    "        for j in range(1,i+1):\n",
    "            print(j,end = \"\")\n",
    "        print(\"\\n\")\n",
    "    return None\n",
    "\n",
    "Increasing_Number_Pattern(5)"
   ]
  }
