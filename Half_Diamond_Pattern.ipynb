   ],
   "source": [
    "def half_diamond_star(n):\n",
    "    for i in range(2 * n):\n",
    "        stars = i\n",
    "        if i > n:\n",
    "            stars = 2 * n - i\n",
    "        print(\"*\" * stars)\n",
    "    print(\"\\n\")\n",
    "    return None\n",
    "\n",
    "half_diamond_star(10)"
   ]
